<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0" />

    <meta name="keywords" content="<%=page.keywords%>">
    <meta name="description" content="<%=page.description%>">
    <meta name="viewport" content="width=device-width,initial-scale=1">

    <!-- CSS -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="<%=static%>/css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection" />
    <link href="<%=static%>/css/style.css" type="text/css" rel="stylesheet" media="screen,projection" />

    <title>
        <%=page.title%> | 
        <%= (page.parent != page.root) ? (page.parent.title + ' | ') : '' %>
        <%= page.root.title %>
    </title>

    <link rel="shortcut icon" href="<%=static%>/favicon.ico" type="image/x-icon">
    <link rel="icon" href="<%=static%>/favicon.ico" type="image/x-icon">

    <script src="<%=cstatic%>/js/lib/jquery-1.9.1<%=min%>.js"></script>
    <script src="<%=cstatic%>/js/lib/jquery.tools.min.js"></script>
    <script src="<%=cstatic%>/js/lib/jquery.validate<%=min%>.js"></script>
    <script src="<%=cstatic%>/js/lib/localization/messages_<%=page.language%>.js"></script>

    <script src="<%=static%>/js/script.js"></script>

    <% var analytics = page.root.getContent("Google Analytics code"); %>
    <% if (typeof analytics !== "undefined") { %>
    <script type="text/javascript">
    var _gaq = _gaq || [];
    _gaq.push(['_setAccount', '<%=analytics.data%>']); // UA-xxxxxxx-x
    _gaq.push(['_trackPageview']);

    (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
    })();

    </script>
    <% } %>

    <!--[if lt IE 9]>
    <script src="<%=cstatic%>/js/lib/html5shiv.js"></script>
    <![endif]-->

    <!--[if lt IE 7 ]>
    <script src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.2/CFInstall.min.js"></script>
    <script>window.attachEvent("onload",function(){CFInstall.check({mode:"overlay"})})</script>
    <![endif]-->
</head>