<% include header.ejs %>

<body id="page<%= page.itemId %>">
  <div id="wrapper">
  
    <div id="main">
      <section>
        <h1><%= page.title %></h1>
      </section>
    </div>

    <div>
      <form method="POST" id="GcalHref" action="/<%=page.language%>/<%=page.itemId%>">
      <input type="hidden" name="request" value="submit">
        <div>
          <label>Your Name</label>
          <input type="text" default="anonymous" name="uName">
        </div>
        <div>
          <label>Your Google Calendar iframe href link</label>
          <input type="text" name="GcalHref" id="GcalHref">
        </div>
        <div>
          <button id="doSubmit">Submit</button>
        </div>
      </form>
    </div>

    <div class="user-info">
      <h4>Calendar for user</h4>
    </div>

    <div class="cal-holder">
      <div>
      <%= calendar[0].href %>
      </div>
  </div>

<% include footer.ejs %>
</div>

</body>
</html>